{
    "name": "kirschbaum-development/nova-chartjs",
    "description": "chart.js Chart Field for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "chart.js"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.12",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "KirschbaumDevelopment\\NovaChartjs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KirschbaumDevelopment\\NovaChartjs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KirschbaumDevelopment\\NovaChartjs\\NovaChartjsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
